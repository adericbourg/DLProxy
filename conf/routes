GET         /download/:url        controllers.DownloadController.index(url)

GET         /                     controllers.ProxyController.index
GET         /proxy/:url           controllers.ProxyController.proxy(url)
POST        /proxy_render         controllers.ProxyController.serveProxied

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file         controllers.Assets.versioned(path="/public", file: Asset)
